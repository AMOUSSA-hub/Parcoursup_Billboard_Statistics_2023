<main>
<mypath path={this.path} ></mypath>
 
<div class="columns">  
<menu class="column" SendForm={this.addTab} UpdatePath={this.UpdatePath} blabla={blalba} ></menu> <etablissement_tab class="column" if ={this.formation !== ""} formation={this.formation} count={this.count} ></etablissement_tab>
 
</div>

        <script>

      

            export default {
                path :[],
                formation: "",
                count: "",
                
                
                onMounted(){
                    console.log(this.props)
                    riot.mount("menu",{UpdatePath:this.UpdatePath,SendForm:this.addTab})
            
                    
                },
                


                UpdatePath(p){
                    this.path = p
                    this.update()


                },

             addTab(f, c){
                
                if(f != this.formation){
               
                    this.formation = f
                    this.count = c
                    this.update()
                   
                    
                    
                    

                }
               

            
            }

                
            }

            

          

            
           
           
        </script>


</main>